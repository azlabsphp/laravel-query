{
    "name": "drewlabs/laravel-query",
    "description": "Provides drewlabs/query bindings for laravel framework",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "azandrew-sidoine",
        "email": "asmyns.platonnas29@gmail.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2|^7.3|^7.4|^8.0|^8.1|^8.2",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/pagination": "^7.0|^8.0|^9.0|^10.0",
        "doctrine/dbal": "^2.0|^3.1",
        "drewlabs/contracts": "^0.2",
        "drewlabs/core-helpers": "^0.2",
        "drewlabs/support": "^0.2",
        "drewlabs/query": "^0.2.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpbench/phpbench": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Drewlabs\\LaravelQuery\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/proxies.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\LaravelQuery\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\LaravelQuery\\ServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "v0.2.0.x-dev"
        }
    }
}
