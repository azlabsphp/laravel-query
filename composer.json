{
    "name": "drewlabs/database",
    "description": "Provides functionnalities for accessing and working with databases using laravel Eloquent, or NoSQL Datatabase providers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "illuminate/contracts": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/pagination": "^5.0|^6.0|^7.0|^8.0",
        "drewlabs/contracts": "^1.0",
        "drewlabs/core": "^1.0",
        "drewlabs/core-helpers": "^1.0",
        "doctrine/dbal": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpbench/phpbench": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Drewlabs\\Packages\\Database\\": "src/"
        },
        "files": [
            "helpers/query.php",
            "helpers/parsers.php",
            "helpers/utils.php",
            "helpers/transformers.php",
            "src/proxies.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Packages\\Database\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-contracts.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core-helpers.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\Packages\\Database\\ServiceProvider"
            ],
            "aliases": {}
        },
        "branch-alias": {
            "dev-master": "v3.0.x-dev"
        }
    }
}
